<%= search_form_for [:admin, @q], class: "form-inline form_search mb-4" do |f| %>
    <%= f.search_field :name_or_description_cont, class: "form-control mr-sm-2", placeholder: t("form.place_name_or_description") %>
    <%= f.search_field :price_gteq, class: "form-control mr-sm-2", placeholder: t("product.price_from") %>
    <%= f.search_field :price_lteq, class: "form-control mr-sm-2", placeholder: t("product.price_to") %>
    <%= f.submit t("page.search"), class: "btn btn-success" %>
<% end %>
